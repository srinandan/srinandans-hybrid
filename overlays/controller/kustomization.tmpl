apiVersion: kustomize.config.k8s.io/v1beta1
kind: Kustomization
images:
- name: ${HUB}/apigee-kube-rbac-proxy
  newTag: v0.8.0
- name: ${HUB}/apigee-operators
  newTag: ${TAG}
patches:
- path: controller.yaml
  target:
    group: apps
    kind: Deployment
    name: apigee-controller-manager
    version: v1
resources:
- ../../base/controller
